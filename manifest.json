{
  "manifest_version": 3,
  "name": "YouTube Q&A with ChatGPT",
  "version": "1.0",
  "description": "Ask ChatGPT questions about YouTube videos.",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["dist/assets/index-Dsn5_NRJ.js"],
      "css": ["dist/assets/index-BqXORsK7.css"]
    }
  ],
  
  "permissions": [
    "activeTab", 
    "scripting", 
    "storage"
  ],
  

  
  "action": {
    "default_popup": "dist/index.html"
  }
  
}
